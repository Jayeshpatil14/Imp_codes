package util;

class Singleton {
	private static SingletonProgram instance;
	public String str;
	private Singleton() {
		String str = "jayesh";
	}
	
	public static Singleton getInstance() {
		if(instance == null) {
			instance = new Singleton();
		}
		return instance;
	}
}

public class SingletonProgram{
	public static void main(String[] args) {
		SingletonProgram s = SingletonProgram.getInstance();
		System.out.println(s.str);
		//System.out.println(s.toString());
	}
}
